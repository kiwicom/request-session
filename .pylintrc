[MASTER]
# jobs=0 means 'use all CPUs'
jobs=0

[MESSAGES CONTROL]
disable =
  missing-docstring,
  line-too-long,
  invalid-name,
  no-value-for-parameter,
  no-member,
  unused-argument,
  broad-except,
  wrong-import-position,
  bare-except,
  locally-disabled,
  protected-access,
  abstract-method,
  fixme,
  too-few-public-methods,
  useless-object-inheritance,
  too-many-arguments,
  too-many-locals,
  too-many-branches,
  attribute-defined-outside-init,
  too-many-instance-attributes,
  unused-import,
  too-many-function-args,
[REPORTS]
output-format=colorized

[FORMAT]
logging-modules=
  logging,
  structlog,
